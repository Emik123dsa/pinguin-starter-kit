{
    "xliffmergeOptions": {
        "srcDir": "apps/jira-client/src",
        "genDir": "apps/jira-client/src/locale",
        "i18nFile": "locale/messages.xlf",
        "i18nBaseFile": "messages",
        "i18nFormat": "xlf",
        "encoding": "UTF-8",
        "defaultLanguage": "en-US",
        "languages": [
            "en",
            "de",
            "Ru"
        ],
        "removeUnusedIds": true,
        "supportNgxTranslate": false,
        "ngxTranslateExtractionPattern": "@@|ngx-translate",
        "useSourceAsTarget": true,
        "targetPrefix": "",
        "targetSuffix": "",
        "beautifyOutput": false,
        "preserveOrder": true,
        "allowIdChange": false,
        "autotranslate": false,
        "apikey": "",
        "apikeyfile": "",
        "verbose": false,
        "quiet": false
    }
}