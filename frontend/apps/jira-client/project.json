{
  "projectType": "application",
  "root": "apps/jira-client",
  "sourceRoot": "apps/jira-client/src",
  "prefix": "pinguin",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/jira-client",
        "index": "apps/jira-client/src/index.html",
        "main": "apps/jira-client/src/main.ts",
        "polyfills": "apps/jira-client/src/polyfills.ts",
        "tsConfig": "apps/jira-client/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/jira-client/src/favicon.ico",
          "apps/jira-client/src/assets",
          "apps/jira-client/src/manifest.webmanifest"
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "apps/jira-client/src/assets/stylesheets"
          ]
        },
        "styles": [
          {
            "input": "apps/jira-client/src/modules/global/tailwindcss/tailwindcss-module.scss",
            "inject": true,
            "bundleName": "tailwindcss-module"
          },
          "apps/jira-client/src/styles.scss"
        ],
        "scripts": [],
        "serviceWorker": true,
        "ngswConfigPath": "apps/jira-client/ngsw-config.json",
        "webWorkerTsConfig": "apps/jira-client/tsconfig.worker.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/jira-client/src/environments/environment.ts",
              "with": "apps/jira-client/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "hmr": {
          "fileReplacements": [
            {
              "replace": "apps/jira-client/src/environments/environment.ts",
              "with": "apps/jira-client/src/environments/environment.hmr.ts"
            }
          ]
        },
        "mock": {
          "fileReplacements": [
            {
              "replace": "apps/jira-client/src/environments/environment.ts",
              "with": "apps/jira-client/src/environments/environment.mock.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "jira-client:build:production",
          "proxyConfig": "apps/jira-client/src/proxy.conf.json"
        },
        "development": {
          "browserTarget": "jira-client:build:development",
          "proxyConfig": "apps/jira-client/src/proxy.conf.json"
        },
        "hmr": {
          "browserTarget": "jira-client:build:hmr",
          "hmr": true,
          "proxyConfig": "apps/jira-client/src/proxy.conf.json"
        },
        "mock": {
          "browserTarget": "jira-client:build:mock"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "jira-client:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/jira-client/src/**/*.ts",
          "apps/jira-client/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/jira-client"
      ],
      "options": {
        "jestConfig": "apps/jira-client/jest.config.js",
        "passWithNoTests": true,
        "webWorkerTsConfig": "apps/jira-client/tsconfig.worker.json"
      }
    }
  },
  "tags": []
}